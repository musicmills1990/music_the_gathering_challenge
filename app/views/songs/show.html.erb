<h1><%= @song.name %></h1>
<h4><i><%= @song.category %></i></h4>
<%= button_to "Delete", song_path(@song), :method => "delete" %>
<h2>Characters Who know this song:</h2>
<% @song.characters.each do |character| %>
  <%= character.name %><br>
<% end %>
<h3><%= link_to "Add Another Song", new_song_path%></h3>
